<%= form_with(model: admin_tax_rule, local: true) do |form| %>
  <div class="form-group">
    <label>name </label>
    <%= form.text_field :name,class: "form-control" %>
  </div>

  <div class="form-group">
    <label> status </label>
    <%= form.select :status, %w{Active Inactive}, :prompt => 'Select', required: true, placeholder: 'status', class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= form.submit %>
  </div>
<% end %>
