<p id="notice"><%= notice %></p>

<h1>Admin Trainers</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Lastname</th>
      <th>Designation</th>
      <th>Organization</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zip code</th>
      <th>Country</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Trainer expertise</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_trainers.each do |admin_trainer| %>
      <tr>
        <td><%= admin_trainer.first_name %></td>
        <td><%= admin_trainer.lastname %></td>
        <td><%= admin_trainer.designation %></td>
        <td><%= admin_trainer.organization %></td>
        <td><%= admin_trainer.address %></td>
        <td><%= admin_trainer.city %></td>
        <td><%= admin_trainer.state %></td>
        <td><%= admin_trainer.zip_code %></td>
        <td><%= admin_trainer.country %></td>
        <td><%= admin_trainer.email %></td>
        <td><%= admin_trainer.phone %></td>
        <td><%= admin_trainer.trainer_expertise %></td>
        <td><%= link_to 'Show', admin_trainer %></td>
        <td><%= link_to 'Edit', edit_admin_trainer_path(admin_trainer) %></td>
        <td><%= link_to 'Destroy', admin_trainer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Trainer', new_admin_trainer_path %>
