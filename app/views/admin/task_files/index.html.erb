<p id="notice"><%= notice %></p>

<h1>Admin Task Files</h1>

<table>
  <thead>
    <tr>
      <th>Attachment</th>
      <th>Admin employee</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_task_files.each do |admin_task_file| %>
      <tr>
        <td><%= admin_task_file.attachment %></td>
        <td><%= admin_task_file.admin_employee_id %></td>
        <td><%= link_to 'Show', admin_task_file %></td>
        <td><%= link_to 'Edit', edit_admin_task_file_path(admin_task_file) %></td>
        <td><%= link_to 'Destroy', admin_task_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Task File', new_admin_task_file_path %>
